# Azure DevOps Configuration
# Copy this file to .env and fill in your values

# Personal Access Token (PAT)
# Create one at: https://dev.azure.com/{org}/_usersSettings/tokens
# Required scope: Code (Read)
AZURE_DEVOPS_PAT=your-pat-token-here

# Default values (can be overridden with command-line args)
AZURE_DEVOPS_ORG=your-organization
AZURE_DEVOPS_PROJECT=your-project
AZURE_DEVOPS_REPO=your-repository
